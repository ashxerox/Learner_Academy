<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>GetStudent</servlet-name>
  	<display-name>GetStudent</display-name>
  	<description></description>
  	<servlet-class>com.learner.web.GetStudent</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.learner.login.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GetStudent</servlet-name>
  	<url-pattern>/getStudent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/loginServlet</url-pattern>
  </servlet-mapping>
</web-app>
